{
    "name": "ngx-property-grid",
    "description": "A small and simple property grid in angular to view/edit POJOs, excellent if you have a \"settings\" object you want to give the user to edit.",
    "version": "1.0.13",
    "private": false,
    "authors": [
        "mokeyish"
    ],
    "keywords": [
        "angular",
        "property grid",
        "ngx"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build-dist": "tsc -p tsconfig-release.json",
        "build":"ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "doc:build": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ./docs",
        "doc:buildandserve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ./docs -s"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mokeyish/ngx-property-grid.git"
    },
    "dependencies": {
        "ngx-template": "^1.0.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.3",
        "@angular/animations": "^6.0.2",
        "@angular/cdk": "^6.1.0",
        "@angular/cli": "~6.0.3",
        "@angular/common": "^6.0.2",
        "@angular/compiler": "^6.0.2",
        "@angular/compiler-cli": "^6.0.2",
        "@angular/core": "^6.0.2",
        "@angular/forms": "^6.0.2",
        "@angular/http": "^6.0.2",
        "@angular/language-service": "^6.0.2",
        "@angular/material": "^6.1.0",
        "@angular/platform-browser": "^6.0.2",
        "@angular/platform-browser-dynamic": "^6.0.2",
        "@angular/router": "^6.0.2",
        "@compodoc/compodoc": "^1.1.3",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.2.1",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ngx-json-viewer": "^2.2.0",
        "protractor": "~5.3.0",
        "rxjs": "^6.0.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.2",
        "zone.js": "^0.8.26"
    }
