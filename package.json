{
  "name": "ngx-property-grid",
  "description": "A small and simple property grid in angular to view/edit POJOs, excellent if you have a \"settings\" object you want to give the user to edit.",
  "version": "9.0.5",
  "private": false,
  "authors": [
    "mokeyish"
  ],
  "keywords": [
    "angular",
    "property grid",
    "ngx",
    "angular9"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mokeyish/ngx-property-grid.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-lib-prod": "ng serve --configuration=lib-prod",
    "build": "ng build",
    "build-lib": "ng build ngx-property-grid --prod",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "publish-npm": "npm run build-lib && npm publish dist/ngx-property-grid",
    "doc:build": "./node_modules/.bin/compodoc -p projects/ngx-property-grid/tsconfig.lib.json -d ./docs && ng build --aot --output-path docs/demo --base-href /demo/",
    "doc:buildandserve": "./node_modules/.bin/compodoc -p projects/ngx-property-grid/tsconfig.lib.json -d ./docs -s"
  },
  "peerDependencies": {
    "@angular/common": "^9.1.4",
    "@angular/core": "^9.1.4"
  },
  "dependencies": {
    "ngx-template": "^1.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular-devkit/build-ng-packagr": "~0.901.4",
    "@angular/animations": "~9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/cli": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-json-view": "^3.0.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "rxjs": "~6.5.4",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  }
}
